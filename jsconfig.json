{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "module": "commonjs",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "layouts/*": ["./src/layouts/*"],
      "projects/*": ["./src/projects/*"],
      "plugins/*": ["./src/plugins/*"],
      "utils/*": ["./src/utils/*"],
      "testConcept/*": ["./src/testConcept/*"],
      "pages/*": ["./src/pages/*"],
      "routers/*": ["./src/routers/*"]
    }
  },
  "exclude": ["node_modules", "build"]
}
